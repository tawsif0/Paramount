{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-tap-highlight-color",
          "text-align: -webkit-match-parent",
          "font-smooth",
          "text-size-adjust",
          "user-select",
          "overscroll-behavior-y",
          "-webkit-text-size-adjust",
          "scale",
          "min-height: auto",
          "rotate",
          "translate"
        ]
      }
    ],
    "css-prefix-order": "off",
    "axe/aria": [
      "default",
      {
        "aria-required-parent": "off"
      }
    ],
    "disown-opener": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "div[translate]"
        ]
      }
    ]
  }
}